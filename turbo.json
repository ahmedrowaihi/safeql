{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "setup": {
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build", "setup"],
      "outputs": ["lib/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [""],
      "inputs": ["src/**/*.ts", "test/**/*.ts"]
    },
    "lint": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint!": {
      "outputs": []
    },
    "watch": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
